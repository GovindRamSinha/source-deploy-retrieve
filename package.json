{
  "name": "source-deploy-retrieve",
  "description": "JavaScript library to run Salesforce metadata deploys and retrieves",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/forcedotcom/source-deploy-retrieve#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forcedotcom/source-deploy-retrieve.git"
  },
  "bugs": {
    "url": "https://github.com/forcedotcom/source-deploy-retrieve/issues"
  },
  "type": "module",
  "keywords": [
    "Salesforce",
    "SalesforceDX",
    "metadata",
    "deploy",
    "retrieve"
  ],
  "engines": {
    "node": ">=12.11.0"
  },
  "dependencies": {
    "@salesforce/core": "2.25.1",
    "@salesforce/kit": "^1.5.0",
    "@salesforce/ts-types": "^1.4.2",
    "archiver": "^5.3.0",
    "fast-xml-parser": "^3.17.4",
    "gitignore-parser": "0.0.2",
    "ignore": "^5.1.8",
    "mime": "2.4.6",
    "unzipper": "0.10.11",
    "xmldom-sfdx-encoding": "^0.1.29"
  },
  "devDependencies": {
    "@commitlint/cli": "^7",
    "@commitlint/config-conventional": "^7",
    "@salesforce/dev-scripts": "^0.6.2",
    "@salesforce/ts-sinon": "^1.1.2",
    "@types/archiver": "^5.1.1",
    "@types/chai": "^4",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/mime": "2.0.3",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "^5",
    "@types/node": "^12.11.0",
    "@types/sinon": "^7.5.2",
    "@types/unzipper": "^0.10.3",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "chai": "^4",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "deep-equal-in-any-order": "^1.1.4",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-jsdoc": "^19.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^5",
    "mocha-junit-reporter": "^1.23.3",
    "prettier": "2.0.5",
    "shelljs": "0.8.4",
    "shx": "^0.3.2",
    "sinon": "^7.3.1",
    "source-map-support": "^0.5.16",
    "typedoc": "~0.18.0",
    "typescript": "3.9.7"
  },
  "scripts": {
    "build": "lerna run build",
    "ci-docs": "yarn sf-ci-docs",
    "commit-init": "commitizen init cz-conventional-changelog --save-dev --save-exact --force",
    "commit": "git-cz",
    "docs": "./scripts/sf-docs.js",
    "format": "lerna run format",
    "lint": "lerna run lint",
    "repl": "node --inspect ./packages/source-deploy-retrieve/scripts/repl.js",
    "test": "lerna run test",
    "package": "lerna run package"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./{src,test}/**/*.{ts,js}": [
      "eslint -c .eslintrc.json --fix"
    ]
  },
  "volta": {
    "node": "12.11.0",
    "yarn": "1.22.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
